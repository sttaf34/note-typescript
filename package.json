    "bcrypt": "^5.0.0",
    "big.js": "^5.2.2",
    "bignumber.js": "^9.0.1",
    "chromedriver": "84.0.1",
    "cookies": "^0.8.0",
    "csv": "^5.3.2",
    "csv-parse": "^4.11.1",
    "dayjs": "^1.9.7",
    "deep-equal": "^2.0.3",
    "dotenv": "^8.2.0",
    "form-data": "^3.0.0",
    "graphql": "^15.3.0",
    "json-server": "^0.16.3",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.28",
    "mysql": "^2.18.1",
