    "@types/jest": "^26.0.7",
    "@types/moment-timezone": "^0.5.13",
    "@types/mysql": "^2.15.15",
    "@types/node": "^14.0.26",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/pug": "^2.0.4",
    "@types/selenium-webdriver": "^4.0.9",
