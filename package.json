    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.3",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript-json-schema": "^0.42.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
